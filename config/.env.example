# MITRE ATT&CK Navigator Application Configuration

# Application Settings
APP_NAME="MITRE ATT&CK Navigator API"
APP_VERSION="1.0.0"
DEBUG=false
LOG_LEVEL=INFO

# API Settings
API_PREFIX="/api/v1"

# CORS Settings (comma-separated origins)
CORS_ORIGINS=http://localhost:4200,http://localhost:4201,http://localhost:8080

# ReliaQuest GreyMatter API
# Get your API key from: https://apidocs.myreliaquest.com/
RELIAQUEST_API_KEY=your-api-key-here
RELIAQUEST_API_URL=https://api.myreliaquest.com/graphql

# Development Settings
# Set to true to use mock data instead of real API calls
USE_MOCK_DATA=true

# Navigator Settings
DEFAULT_DOMAIN=enterprise-attack
CACHE_TTL=300
